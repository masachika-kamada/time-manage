import win32gui, win32process, psutil
import time

def active_window_process_name():
    try:
        pid = win32process.GetWindowThreadProcessId(win32gui.GetForegroundWindow())
        return(psutil.Process(pid[-1]).name())
    except:
        pass

for i in range(10):
    print(active_window_process_name())
    time.sleep(1)